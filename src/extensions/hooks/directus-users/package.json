{
	"name": "directus-users",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook"
	],
	"type": "module",
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.3.3"
	},
	"scripts": {
		"build": "directus-extension build --no-minify",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"test": "tsc --noEmit && NODE_ENV=test mocha",
		"test:dev": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=true mocha"
	},
	"devDependencies": {
		"@directus/extensions": "^1.0.9",
		"@directus/extensions-sdk": "11.0.9",
		"@directus/types": "^11.2.0",
		"@types/chai": "^4.3.17",
		"@types/mocha": "^10.0.7",
		"@types/node": "^22.13.14",
		"@types/sinon": "^17.0.4",
		"chai": "^5.1.1",
		"mocha": "^10.7.0",
		"sinon": "^18.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@directus/errors": "^0.3.3",
		"@isaacs/ttlcache": "^1.4.1",
		"joi": "^18.0.1"
	}
}
