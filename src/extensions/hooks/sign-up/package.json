{
	"name": "sign-up",
	"description": "Hook that manages user properties during registration",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook"
	],
	"type": "module",
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.1.1"
	},
	"scripts": {
		"build": "directus-extension build --no-minify",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"test": "tsc --noEmit && NODE_ENV=test mocha",
		"test:dev": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=true mocha"
	},
	"devDependencies": {
		"@directus/extensions": "^3.0.11",
		"@directus/extensions-sdk": "16.0.2",
		"@directus/types": "^13.2.3",
		"@types/chai": "^5.2.2",
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.13.14",
		"@types/sinon": "^17.0.4",
		"chai": "^6.2.0",
		"mocha": "^11.7.4",
		"sinon": "^18.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@directus/errors": "^2.0.4",
		"@isaacs/ttlcache": "^1.4.1",
		"@scure/base": "^2.0.0",
		"axios": "^1.12.2",
		"axios-retry": "^4.5.0",
		"nock": "^14.0.10"
	}
}
