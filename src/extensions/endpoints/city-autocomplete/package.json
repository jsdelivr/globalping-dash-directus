{
	"name": "city-autocomplete",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-endpoint"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"download:files": "tsx src/download.ts",
		"build": "pnpm run download:files && directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"test": "tsc --noEmit && NODE_ENV=test mocha",
		"test:dev": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=true mocha"
	},
	"devDependencies": {
		"@directus/extensions": "^3.0.11",
		"@directus/extensions-sdk": "16.0.2",
		"@directus/types": "^13.2.3",
		"@types/adm-zip": "^0.5.7",
		"@types/chai": "^5.2.2",
		"@types/express": "^4.17.21",
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.13.14",
		"@types/sinon": "^17.0.4",
		"@types/supertest": "^6.0.3",
		"chai": "^6.2.0",
		"mocha": "^11.7.4",
		"nock": "^14.0.10",
		"sinon": "^18.0.1",
		"supertest": "^7.1.4",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.6",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@directus/errors": "^2.0.4",
		"adm-zip": "^0.5.16",
		"any-ascii": "^0.3.3",
		"axios": "^1.12.2",
		"countries-list": "^3.1.1",
		"csv-parser": "^3.2.0",
		"express": "^4.21.2",
		"joi": "^18.0.1",
		"lodash": "^4.17.21"
	}
}
