{
	"name": "adoption-code",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-endpoint"
	],
	"type": "module",
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.1.11"
	},
	"scripts": {
		"build": "directus-extension build --no-minify",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"test": "tsc --noEmit && NODE_ENV=test mocha",
		"test:dev": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=true mocha"
	},
	"devDependencies": {
		"@directus/extensions": "^3.0.11",
		"@directus/extensions-sdk": "16.0.2",
		"@directus/types": "^13.2.3",
		"@types/chai": "^5.2.2",
		"@types/express": "^4.17.21",
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.13.14",
		"@types/sinon": "^17.0.4",
		"@types/supertest": "^6.0.3",
		"chai": "^6.2.0",
		"express": "^4.21.2",
		"mocha": "^11.7.4",
		"nock": "^13.5.4",
		"sinon": "^18.0.1",
		"supertest": "^7.1.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@directus/errors": "^2.0.4",
		"@isaacs/ttlcache": "^1.4.1",
		"axios": "^1.12.2",
		"countries-list": "^3.1.1",
		"ipaddr.js": "^2.2.0",
		"joi": "^18.0.1",
		"lodash": "^4.17.21",
		"rate-limiter-flexible": "^8.0.1"
	}
}
